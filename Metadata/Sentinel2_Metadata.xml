               <gmd:title>
                  <gco:CharacterString>Sentinel-2 data used in OBIA4RTM</gco:CharacterString>
               </gmd:title>
               <gmd:date>
                  <gmd:CI_Date>
                     <gmd:date>
                        <gco:Date>2020-01-28</gco:Date>
                     </gmd:date>
                     <gmd:dateType>
